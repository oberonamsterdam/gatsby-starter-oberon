{
    "name": "gatsby-starter-oberon",
    "description": "A Gatsby starter with typesafe GraphQL using TypeScript and Oberon defaults",
    "version": "0.1.0",
    "author": "Oberon <devs@oberon.nl>",
    "scripts": {
        "build": "gatsby build",
        "format": "prettier --write '{__mocks__,src,test}/**/*.{js,ts,tsx}'",
        "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
        "clean": "rimraf .cache public",
        "typecheck": "tsc --project . --noEmit",
        "start": "gatsby develop",
        "serve": "gatsby serve",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
        "codegen": "graphql-codegen --config graphql-codegen.yml"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "gatsby": "^2.19.32",
        "gatsby-image": "^2.2.43",
        "gatsby-plugin-graphql-preview": "^2.1.0",
        "gatsby-plugin-manifest": "^2.2.44",
        "gatsby-plugin-offline": "^3.0.37",
        "gatsby-plugin-react-helmet": "^3.1.23",
        "gatsby-plugin-sentry": "^1.0.1",
        "gatsby-plugin-sharp": "^2.4.7",
        "gatsby-plugin-styled-components": "^3.1.20",
        "gatsby-plugin-typescript": "^2.2.2",
        "gatsby-source-filesystem": "^2.1.50",
        "gatsby-source-graphql": "^2.1.34",
        "gatsby-transformer-sharp": "^2.3.17",
        "graphql": "^14.6.0",
        "graphql-tag": "^2.10.3",
        "modern-normalize": "^0.6.0",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-helmet": "^5.2.1",
        "styled-components": "^5.0.1"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^1.12.2",
        "@graphql-codegen/introspection": "^1.12.2",
        "@graphql-codegen/typescript": "^1.12.2",
        "@graphql-codegen/typescript-operations": "^1.12.2",
        "@types/node": "^13.9.0",
        "@types/react": "^16.9.23",
        "@types/react-dom": "^16.9.5",
        "@types/react-helmet": "^5.0.15",
        "@types/styled-components": "^5.0.1",
        "babel-plugin-styled-components": "^1.10.7",
        "eslint": "^6.8.0",
        "eslint-config-oberon": "^2.0.0",
        "husky": "^4.2.3",
        "lint-staged": "^10.0.8",
        "oberon-prettier-config": "^1.0.0",
        "prettier": "^1.19.1",
        "typescript": "^3.8.3",
        "typescript-styled-plugin": "^0.15.0"
    },
    "keywords": [
        "gatsby",
        "typescript",
        "graphql",
        "oberon"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/oberonamsterdam/gatsby-starter-oberon"
    }
}
